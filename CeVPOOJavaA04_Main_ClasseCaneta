package Aula04;

public class Aula04Main {

    public static void main(String[] args) {
        Caneta2 c1 = new Caneta2();
        
        c1.setModelo("BIC");
        c1.status();
        c1.setPonta(0.5f);
        
        System.out.println("Tehno uma caneta " + c1.getModelo() + "de ponta " + 
         c1.getPonta());
      
    }
    
}
